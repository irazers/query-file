{
  "name": "query-file",
  "description": "A CLI to query file formats like parquet, json or CSV",
  "version": "0.0.1",
  "author": "Cedric Peruzzi",
  "bin": {
    "qf": "./bin/run.js"
  },
  "bugs": "https://github.com/irazers/query-file/issues",
  "dependencies": {
    "@duckdb/node-api": "1.1.3-alpha.12",
    "@oclif/core": "^4.2.6",
    "@oclif/plugin-help": "^6.2.24",
    "@oclif/plugin-plugins": "^5.4.30",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@oclif/test": "^4.1.9",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.10",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "chai": "^4.5.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "mocha": "^10.8.2",
    "oclif": "^4.17.23",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/irazers/query-file",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "qf",
    "dirname": "qf",
    "commands": {
      "strategy": "pattern",
      "target": "./dist/commands"
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "repository": "https://github.com/irazers/query-file",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
